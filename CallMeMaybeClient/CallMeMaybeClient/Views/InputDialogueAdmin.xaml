<Window x:Class="CallMeMaybeClient.Views.InputDialogueAdmin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Mot de passe" Height="200" Width="300">
    <StackPanel>
        <TextBlock Name="MessageText" Margin="10" />
        <PasswordBox Name="PasswordBox" Margin="10" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="10">
            <Button Content="OK" Name="OkButton" Width="85" Click="OkButton_Click"/>
            <Button Content="Annuler" IsCancel="True"  />
        </StackPanel>
    </StackPanel>
</Window>
